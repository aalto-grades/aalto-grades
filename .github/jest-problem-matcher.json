{
  "problemMatcher": [
    {
      "owner": "jest",
      "pattern": [
        {
          "regexp": "^\\s*(●|✖|✓|◌)\\s+(.*)\\s*\\((\\d+ ms)\\)$",
          "severity": 1, 
          "message": 2,
          "file": 1
        },
        {
          "regexp": "^\\s+●\\s+(.*)$",
          "severity": 1,  
          "message": 1
        },
        {
          "regexp": "^\\s+at\\s+(.*)\\s+\\(([^:]+):(\\d+):(\\d+)\\)$",
          "file": 2,
          "line": 3,
          "column": 4
        }
      ]
    }
  ]
}