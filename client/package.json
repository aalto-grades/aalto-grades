{
  "name": "aalto-grades-client",
  "license": "Expat (MIT)",
  "author": "The Aalto Grades Developers",
  "scripts": {
    "build": "tsc -b && vite build",
    "postinstall": "patch-package",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "preview": "vite preview --port 3005",
    "prod-licenses": "npx license-checker-rseidelsohn --excludePrivatePackages --production --summary",
    "start": "vite",
    "trans-copy": "cp public/locales/en/translation.json ./",
    "trans-merge": "npx tsx translation-utils.ts merge",
    "trans-parse": "npx i18next 'src/**/*.{ts,tsx}' -c i18next-parser.config.js -o 'public/locales/$LOCALE/$NAMESPACE.json'",
    "trans-rename": "npx tsx translation-utils.ts rename"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.1.0",
    "@mui/types": "^7.2.4",
    "@mui/x-data-grid": "^7.3.0",
    "@mui/x-date-pickers": "^7.2.0",
    "@tanstack/react-query": "^5.18.0",
    "@tanstack/react-query-devtools": "^5.18.0",
    "@tanstack/react-table": "^8.10.7",
    "@tanstack/react-virtual": "^3.1.3",
    "@types/spdx-expression-parse": "^3.0.5",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "dayjs": "^1.11.10",
    "dompurify": "^3.1.6",
    "elkjs": "^0.9.1",
    "formik": "^2.4.2",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "mui-one-time-password-input": "^2.0.3",
    "notistack": "^3.0.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-global-modal": "0.0.27",
    "react-i18next": "^15.0.0",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^6.11.2",
    "reactflow": "^11.10.3",
    "styled-components": "^6.1.8",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.16.5",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/readline-sync": "^1.4.8",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "i18next-parser": "^9.0.1",
    "license-checker-rseidelsohn": "^4.2.6",
    "patch-package": "^8.0.0",
    "readline-sync": "^1.4.10",
    "rollup-plugin-license": "^3.2.0",
    "sass": "^1.75.0",
    "spdx-expression-parse": "^4.0.0",
    "tsx": "^4.17.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "engines": {
    "node": ">=20"
  }
}
