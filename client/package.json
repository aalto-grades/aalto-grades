{
  "name": "ossi-client",
  "license": "Expat (MIT)",
  "author": "The Ossi Developers",
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --fix",
    "preview": "vite preview --port 3005",
    "prod-licenses": "npx license-checker-rseidelsohn --excludePrivatePackages --production --summary",
    "start": "vite",
    "trans-copy": "cp public/locales/en/translation.json ./",
    "trans-merge": "npx tsx translation-utils.ts merge",
    "trans-parse": "npx i18next 'src/**/*.{ts,tsx}' -c i18next-parser.config.js -o 'public/locales/$LOCALE/$NAMESPACE.json'",
    "trans-rename": "npx tsx translation-utils.ts rename"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/types": "^7.4.1",
    "@mui/x-data-grid": "^8.1.0",
    "@mui/x-date-pickers": "^8.1.0",
    "@tanstack/react-query": "^5.74.7",
    "@tanstack/react-query-devtools": "^5.74.7",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.6",
    "@types/spdx-expression-parse": "^3.0.5",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.5",
    "elkjs": "^0.10.0",
    "formik": "^2.4.6",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "i18next-http-backend": "^3.0.2",
    "mui-one-time-password-input": "^5.0.0",
    "notistack": "^3.0.2",
    "papaparse": "^5.5.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-global-modal": "github:gubo97000/react-global-modal",
    "react-i18next": "^15.5.1",
    "react-qr-code": "^2.0.15",
    "react-router-dom": "^7.5.2",
    "reactflow": "^11.11.4",
    "styled-components": "^6.1.17",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.15.2",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/readline-sync": "^1.4.8",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "i18next-parser": "^9.3.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "readline-sync": "^1.4.10",
    "rollup-plugin-license": "^3.6.0",
    "sass": "^1.87.0",
    "spdx-expression-parse": "^4.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20"
