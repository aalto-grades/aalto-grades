  "license": "Expat (MIT)",
  "private": true,
  "workspaces": [
    "../common"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.3",
    "@mui/types": "^7.2.4",
    "@mui/x-data-grid": "^6.6.0",
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/styled-components": "^5.1.26",
    "aalto-grades-common": "*",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12",
    "formik": "^2.4.2",
    "msw": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yup": "^1.2.0"
  },
  "scripts": {
    "start": "npm --prefix ../common run build && DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "./build.sh",
    "test": "DISABLE_ESLINT_PLUGIN=true react-scripts test",
    "eject": "react-scripts eject",
    "lint": "npx eslint `find . -name '*.tsx' -not -path './node_modules/*'`",
    "lint:fix": "npx eslint `find . -name '*.tsx' -not -path './node_modules/*'` --fix",
    "prod-licenses": "npx license-checker-rseidelsohn --excludePrivatePackages --production --summary"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.27.0",
    "eslint-plugin-react": "^7.31.10",
    "license-checker-rseidelsohn": "^4.2.6",
    "spdx-expression-parse": "^3.0.1",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)/"
    ]
  }
}
