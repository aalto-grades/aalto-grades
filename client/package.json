  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "old-start": "npm --prefix ../common run build && DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "old-build": "./build.sh",
    "test": "DISABLE_ESLINT_PLUGIN=true react-scripts test",
    "vtest": "jest",
    "vitest": "vitest run",
    "eject": "react-scripts eject",
    "lint": "npx eslint `find . -name '*.tsx' -not -path './node_modules/*'`",
    "lint:fix": "npx eslint `find . -name '*.tsx' -not -path './node_modules/*'` --fix",
    "prod-licenses": "npx license-checker-rseidelsohn --excludePrivatePackages --production --summary"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/parser": "^5.59.8",
