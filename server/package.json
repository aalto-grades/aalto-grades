{
  "name": "aalto-grades-server",
  "author": "The Aalto Grades Developers",
  "license": "Expat (MIT)",
  "description": "",
  "main": "",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node build/server/src/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "test": "jest --verbose --detectOpenHandles --coverage",
    "citest": "GITHUB_ACTIONS=1 jest --detectOpenHandles",
    "lint": "eslint . --ext ts --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts --report-unused-disable-directives --fix",
    "migration:up": "npx sequelize-cli db:migrate",
    "migration:down": "npx sequelize-cli db:migrate:undo",
    "migration:down:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:down": "npx sequelize-cli db:seed:undo"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@node-saml/passport-saml": "^4.0.4",
    "@xmldom/xmldom": "^0.8.10",
    "argon2": "^0.40.3",
    "axios": "^1.2.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-stringify": "^6.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "generate-password": "^1.7.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "rate-limiter-flexible": "^5.0.3",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.5.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.10.0",
    "xpath": "^0.0.34",
    "zod": "^3.22.4",
    "zod-express-middleware": "^1.4.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.9",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.34",
    "@types/pg": "^8.11.5",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "cookiejar": "^2.1.4",
    "eslint": "^8.27.0",
    "jest": "^29.3.1",
    "mockdate": "^3.0.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
